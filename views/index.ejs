<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Account - List</h1>
    <table border="1" >
        <tr>
            <th>id</th>
            <th>email</th>
            <th>password</th>
        </tr>
        <% for ( var i = 0 ; i < data.length ; i++){ %>
          <tr>
              <td>
                  <%= data[i].id  %>
              </td>
              <td>
                <%= data[i].email  %>
              </td>
              <td>
                  <%= data[i].password  %>
              </td>
          </tr>
          <% } %>
    </table>
  </body>
</html>
